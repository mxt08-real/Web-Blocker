{
"manifest_version": 2,
"name": "Web Blocker",
"description": "Blocks you browser for other persons",
"version": "1.0",
"icons": {
 "1200": "icon.png" },
"permissions": [
 "<all_urls>",
 "unlimitedStorage",
 "webRequestBlocking" ],
"content_scripts": [ {
 "matches": ["<all_urls>"],
 "js": ["content.js"] } ],
"browser_action": {
 "default_icon": {
  "1200": "icon.png" },
 "default_title": "Web Blocker",
 "default_popup": "popup.html" }
}
